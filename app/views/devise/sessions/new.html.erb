<div class="panel panel-default devise-bs main-form col-md-offset-4 col-md-4 col-sm-offset-2 col-sm-8 col-xs-offset-2 col-xs-8">
  <div class="panel-body">
    <%= form_for(resource, as: resource_name, url: session_path(resource_name), html: { role: 'form' }) do |f| %>
      <h3 class="form-title">Sign In</h3>
      <div class="form-group checkbox-form-label">
        <%= f.email_field :email, autofocus: true, class: 'form-control', placeholder: "Enter Your Email" %>
      </div>
      <div class="form-group checkbox-form-label">
        <%= f.password_field :password, autocomplete: 'off', class: 'form-control', placeholder: "Enter Your Password" %>
      </div>
      <%= f.submit  t('.sign_in', default: 'Sign in'), class: 'btn btn-signin' %>
    <% end %>
  </div>
</div>
